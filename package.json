    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@umijs/plugin-sass": "^1.1.1",
    "cpr": "^3.0.1",
    "dumi": "^1.1.41-rc.0",
